    "redux": "3.6.0",
    "superagent": "2.2.0",
    "babel-polyfill": "6.1.18"
  },
  "devDependencies": {
    "babel-cli": "6.1.18",
    "babel-core": "^6.1.18",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-es2015-modules-commonjs": "6.14.0",
    "babel-preset-airbnb": "^2.0.0",
    "babel-preset-es2015": "6.1.18",
    "babel-preset-react": "6.1.18",
    "babel-preset-stage-0": "6.1.18",
    "babel-register": "6.14.0",
    "chai": "3.5.0",
    "enzyme": "2.4.1",
    "glob": "7.0.6",
    "jsdom": "9.5.0",
    "jsdomify": "3.1.0",
    "mocha": "3.0.2",
    "mocha-jsdom": "1.1.0",
